@Data
public class Lemon {

	private String manthravadiName;
	private String Location;
	int[] nums;
	ArrayList<String> manthravaadiGalu;
	Map<String, String> magicians;
	Properties mantravettas;

	public void mataManthra() {
		System.out.println(manthravadiName + " is famous for maatamantra");
		System.out.println("available mantravaadi's ");
		System.out.println(manthravaadiGalu);
		System.out.println("available mantravaadi's with address");
		System.out.println(magicians);
		System.out.println(mantravettas);
	}

}
